<div class="room-index-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-md-offset-2 col-md-6">
        <h1>DM一覧</h1>  
        <div class="text-center">
          <table class="table">
            <% @anotherEntries.each do |e| %>
              <%= link_to room_path(e.room.id) do %>
                <%= e.user.name %>へメッセージを送る</br><hr>
              <% end %>
            <% end %>
          </table>
        </div>  
      </div>
    </div>
  </div>
</div>